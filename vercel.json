{
  "version": 2,
  "builds": [
    {
      "src": "build.sh",
      "use": "@vercel/node"
    },
    {
      "src": "studentregistration/wsgi.py",
      "use": "@vercel/python",
      "config": {
        "maxLambdaSize": "15mb",
        "runtime": "python3.8"
      }
    }
  ],
  "routes": [
    {
      "src": "/build.sh",
      "dest": "/build.sh"
    },
    {
      "src": "/(.*)",
      "dest": "studentregistration/wsgi.py"
    }
  ]
}
